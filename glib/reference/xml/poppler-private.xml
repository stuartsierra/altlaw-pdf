<title><anchor id="PopplerPSFile" role="struct"/>PopplerPSFile</title>
<indexterm><primary>PopplerPSFile</primary></indexterm><programlisting>typedef struct {
  GObject parent_instance;

  PopplerDocument *document;
  PSOutputDev *out;
  char *filename;
  int first_page;
  int last_page;
  double paper_width;
  double paper_height;
  gboolean duplex;
} PopplerPSFile;
</programlisting>
